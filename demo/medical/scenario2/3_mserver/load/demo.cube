<cube>
  <agent>
    <host value="localhost"/>
    <port value="38010"/>

	<rmcheck-interval value="3000"/>
	<keepalive-interval value="2000"/>
	<keepalive-retry value="2"/>

	<debug value="false"/>

    <archetypeUrl value="file:../scenario2.arch"/>

	<plugins>
		<plugin id="fr.liglab.adele.cube.core"/>
		<plugin id="fr.liglab.adele.cube.osgi">
			<property name="type" value="MediationServer"/>
			<property name="loadbalancing" value="true"/>
			<property name="active" value="false"/>
		</plugin>
		<plugin id="fr.liglab.adele.cube.loadbalancing">
			<property name="interval" value="6000"/>
		</plugin>
		<plugin id="fr.liglab.adele.cube.rm.monitoring"/>
	</plugins>
  </agent>
</cube>




