<cube xmlns:core="fr.liglab.adele.cube.core">
  <archetype id="net.debbabi.cube.demo.connect" name="Connecting Components Tutorial 1" version="1.0">

    <types>
      <!--core:node id="PC"/-->
      <core:component id="A"/>
      <core:component id="B"/>
    </types>

    <constraints>
      <variables>
        <var id="va" type="A"/>
        <var id="vb" type="B"/>
      </variables>
      <core:connect v1="va" v2="vb"/>
    </constraints>

  </archetype>
</cube>
